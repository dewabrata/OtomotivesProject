<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <android.support.design.widget.AppBarLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:theme="@style/AppTheme.AppBarOverlay">

            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="?attr/colorPrimary"
                app:popupTheme="@style/AppTheme.PopupOverlay">

            </android.support.v7.widget.Toolbar>

        </android.support.design.widget.AppBarLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp">

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="ANGKA"
                android:layout_weight="1"/>

            <Button
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="KEUANGAN"
                android:enabled="false"
                android:layout_weight="1"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="@dimen/paddingXXSmall"
            android:gravity="center"
            android:orientation="horizontal"
            android:paddingStart="@dimen/paddingMedium"
            android:paddingEnd="@dimen/paddingMedium">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:textSize="12sp"
                android:text="PERIODE"
                android:textColor="@color/grey_900"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="6"
                android:orientation="horizontal"
                android:padding="@dimen/paddingMedium">

                <TextView
                    android:id="@+id/ic_mulaitgl"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:drawableLeft="@drawable/icon_tanggal"
                    android:textStyle="bold"
                    android:layout_weight="1"
                    tools:ignore="NestedWeights" />

                <TextView
                    android:id="@+id/ic_selesaitgl"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="10dp"
                    android:layout_weight="1"
                    android:drawableRight="@drawable/icon_tanggal"
                    android:textStyle="bold"
                    android:enabled="false"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/paddingXXSmall"
            android:gravity="center"
            android:orientation="horizontal"
            android:paddingStart="@dimen/paddingMedium"
            android:paddingEnd="@dimen/paddingMedium">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:textSize="12sp"
                android:textColor="@color/grey_900"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="6"
                android:background="@drawable/background_edittext"
                android:orientation="horizontal"
                android:padding="@dimen/paddingMedium">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                </LinearLayout>

                <TextView
                    android:id="@+id/tv_mulaitgl"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="4"
                    android:gravity="center"
                    android:textColor="@color/grey_900"
                    android:textSize="12sp"
                    android:hint="DD/MM/YY"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="3dp"
                    android:layout_marginRight="3dp"
                    android:text="-"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_selesaitgl"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="4"
                    android:gravity="center"
                    android:textColor="@color/grey_900"
                    android:textSize="12sp"
                    android:hint="DD/MM/YY"
                    android:textStyle="bold"
                    android:enabled="false"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="15dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="@dimen/paddingSmall"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbBooking"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="NNM"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="BOOKING"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbLayanan1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="NNM"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="LAYANAN"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbJualpart"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="NNM"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="JUAL PART"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbBatal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="NNM"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="BATAL"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>



        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#bbb"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="15dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="@dimen/paddingSmall"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbLayanan2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="LAYANAN"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbPart"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="PART"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbJasapart"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="JASA PART"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="15dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="@dimen/paddingSmall"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbJasalainnya"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="JASA LAINNYA"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dblainnya"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="LAINNYA"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbDiscount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="DISCOUNT"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#bbb"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="15dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="@dimen/paddingSmall"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbPendapatan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="PENDAPATAN"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbDownpayment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="DOWNPAYMENT"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbIncome"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="INCOME"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="15dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="@dimen/paddingSmall"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbBiaya"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="BIAYA"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbHpppart"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="HPP PART"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbMargin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="MARGIN"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#bbb"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="15dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="@dimen/paddingSmall"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbKas"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="KAS"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbBank"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="BANK"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbPiutang"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="PIUTANG"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="15dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="@dimen/paddingSmall"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbColeection"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="COLLECTION"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbStockpart"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="STOCK PART"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:gravity="center">

                <TextView
                    android:id="@+id/tv_dbHutang"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rp. NN.NNN.NNN"
                    android:textSize="10dp"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:text="HUTANG"
                    android:textColor="@color/grey_900"
                    android:textStyle="bold"/>

